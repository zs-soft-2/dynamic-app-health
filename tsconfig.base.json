{
	"compileOnSave": false,
	"compilerOptions": {
		"allowSyntheticDefaultImports": true,
		"rootDir": ".",
		"sourceMap": true,
		"declaration": false,
		"moduleResolution": "node",
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"importHelpers": true,
		"target": "es2015",
		"module": "esnext",
		"lib": ["es2017", "dom"],
		"skipLibCheck": true,
		"skipDefaultLibCheck": true,
		"baseUrl": ".",
		"paths": {
			"@dynamic-app-health/api": ["libs/api/src/index.ts"],
			"@dynamic-app-health/core/application-config/data": [
				"libs/core/application-config/data/src/index.ts"
			],
			"@dynamic-app-health/core/authentication/data": [
				"libs/core/authentication/data/src/index.ts"
			],
			"@dynamic-app-health/core/error/data": [
				"libs/core/error/data/src/index.ts"
			],
			"@dynamic-app-health/core/error/util": [
				"libs/core/error/util/src/index.ts"
			],
			"@dynamic-app-health/core/fhir-client/data": [
				"libs/core/fhir-client/data/src/index.ts"
			],
			"@dynamic-app-health/core/i18n/data": [
				"libs/core/i18n/data/src/index.ts"
			],
			"@dynamic-app-health/domain/fhir/patient/data": [
				"libs/domain/fhir/patient/data/src/index.ts"
			],
			"@dynamic-app-health/domain/fhir/patient/util": [
				"libs/domain/fhir/patient/util/src/index.ts"
			],
			"@dynamic-app-health/domain/fhir/patient/view": [
				"libs/domain/fhir/patient/view/src/index.ts"
			],
			"@dynamic-app-health/feature/dynamic-config/data": [
				"libs/feature/dynamic-config/data/src/index.ts"
			],
			"@dynamic-app-health/feature/dynamic-config/editor": [
				"libs/feature/dynamic-config/editor/src/index.ts"
			],
			"@dynamic-app-health/feature/dynamic-layout/view": [
				"libs/feature/dynamic-layout/view/src/index.ts"
			],
			"@dynamic-app-health/feature/dynamic-page/data": [
				"libs/feature/dynamic-page/data/src/index.ts"
			],
			"@dynamic-app-health/feature/dynamic-page/editor": [
				"libs/feature/dynamic-page/editor/src/index.ts"
			],
			"@dynamic-app-health/feature/dynamic-page/view": [
				"libs/feature/dynamic-page/view/src/index.ts"
			]
		}
	},
	"exclude": ["node_modules", "tmp"]
}
